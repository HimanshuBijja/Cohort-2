generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  age      Int
}

model todo {
  id          Int     @id @default(autoincrement())
  todo        String
  description String
  done        Boolean @default(false)
}

model test {
  id Int @id
}
